@model IEnumerable<OrderFoodOnline.Models.FoodOrder>

@{ ViewData["Title"] = "Home"; }
@{ Layout = "_Layout2"; }
<br />
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FoodName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FoodType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumberOfDishes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.FoodName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.FoodType)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.NumberOfDishes)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Rate)
    </td>
</tr>
}
    </tbody>
</table>
